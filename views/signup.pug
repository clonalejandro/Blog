doctype html
html
    head
        - var title = "SignUp"
        - var description = "A login system developed by clonalejandro"
        - var author = "clonalejandro"
        - var thumb = `${blogLogo}`
        - var tagsString = "login system developed clonalejandro node nodejs js javascript"
        - var twitter = "@clonalejandro11"
        - var blogName = "SignUp"

        meta(name="DC.title", content=`${title}`)
        meta(property="og:title", content=`${title}`)
        meta(name="twitter:title", content=`${title}`)
        meta(charset="utf-8")
        meta(lang="es")
        meta(http-equiv="Content-Language", content="en")
        meta(http-equiv="Content-Type", content="text/html; charset=UTF-8")
        meta(name="description", content=`${description}`)
        meta(name="keywords", content=`${tagsString}`)
        meta(name="author", content=`${author}`)
        link(rel="apple-touch-icon", sizes="120x120", alt=`${title}`, href=`${blogLogo}?size=120`)
        link(rel="apple-touch-icon", sizes="152x152", alt=`${title}`, href=`${blogLogo}?size=152`)
        link(rel="icon", href=`${blogLogo}`, alt=`{title}`, type="image/png")
        link(rel="shortcut icon", href=`${blogLogo}`, alt=`${blogLogo}`, type="image/png")
        meta(name="robots", content="all")
        meta(name="distribution", content="global")
        meta(name="google", content="translate")
        meta(name="DC.creator", content=`${author}`)
        meta(name="DC.description", content=`${description}`)
        meta(property="og:description", content=`${description}`)
        meta(property="og:type", content="website")
        meta(property="og:url", content=`${webURI}`)
        meta(property="og:image", content=`${thumb}`)
        meta(property="og:site_name", content=`${blogName}`)
        meta(name="twitter:card", content="summary")
        meta(name="twitter:site", content=`${twitter}`)
        meta(name="twitter:description", content=`${description}`)
        meta(name="twitter:image", content=`${thumb}`)
        meta(name="twitter:url", content=`${webURI}`)
        meta(name="generator", content="clonacms")
        meta(name="viewport", content="width=device-width, initial-scale=1.0, shrink-to-fit=no")
        meta(name="theme-color", content="#000000")

        title #{title}

        //Cookies
        link(rel="stylesheet", type="text/css", href="//cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.0/cookieconsent.min.css")
        script(async, src="https://cdnjs.cloudflare.com/ajax/libs/cookieconsent2/3.1.0/cookieconsent.min.js")
        script.
            window.addEventListener("load", function(){
            window.cookieconsent.initialise({
                "palette": {
                    "popup": {
                    "background": "#3c404d",
                    "text": "#d6d6d6"
                    },
                    "button": {
                    "background": "transparent",
                    "text": "#8bed4f",
                    "border": "#8bed4f"
                    }
                },
                "position": "bottom-right",
                "content": {
                    "dismiss": "Let's go",
                    "href": "/tos"
                }
            })});
            
        link(href="https://clonalejandro.github.io/cdn/clonaweb/malextrap.min.css", rel="stylesheet")
        style 
            include assets/css/style.min.css
    body
        div.alert.top.danger#alertLogin
            div.content
                img(width="64px", height="64px", alt="warning", src="data:image/svg+xml;utf8;base64,")
                a#alertLoginText #{message}

        div#wrapper
            header.header
                div.title
                    a(href="/")
                        include components/logo.svg
                    h1.headline #{blogName}

            div.container
                form(action="/signup", method="post")
                    div.form-group
                        div#section-email
                            label Email 
                                i.icon.icons-mail
                            input(name="email", type="email")
                        br
                        div#section-user
                            label User 
                                i.icon.icons-user
                            input(name="username", type="text")
                        br
                        div#section-password
                            label Secret 
                                i.icon.icons-lock
                            input(name="password", type="password")
                        br
                        button.btn.adapter.green.animated(type="submit") Validate
                            i.icon.icons-lock-open
        include components/footer.pug
        include components/script.pug
        script
            include assets/js/auth.min.js